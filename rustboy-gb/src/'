use super::{envelope::VolumeEnvelope, length_counter::LengthCounter};

pub struct Square2 {
    vol_envelope: VolumeEnvelope,
    length_counter: LengthCounter,
    duty: u8,
    sequence: i32,
    timer: i32,
    channel_enabled: bool,
    dac_enabled: bool,
    output: u8,
    freq: i32,
}

impl Square2 {

    fn new() -> Square2 {
        Square2 {
            timer: 0,
            sequence: 0, 
            freq: 0, 
            duty: 0,
            channel_enabled: false,
            dac_enabled: false,
            output: false,
            vol_envelope: VolumeEnvelope::new();
        }

    }

}
